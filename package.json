{
  "name": "omnia_lighting_app_frontend",
  "version": "0.2.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "build": "cd src/omnia_lighting_app_frontend && npm run build",
    "prebuild": "npm run generate",
    "start": "cd src/omnia_lighting_app_frontend && npm run dev",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate omnia_lighting_app_backend"
