  "dependencies": {
    "@dfinity/agent": "^0.15.6",
    "@dfinity/candid": "^0.15.6",
    "@dfinity/principal": "^0.15.6"
  },
  "devDependencies": {
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "dotenv": "^16.0.3",
    "events": "3.3.0",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "util": "0.12.4"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
